    "prebuild": "rimraf dist",
    "build:api": "nest build -c api-nest-cli.json",
    "build:console": "nest build -c console-nest-cli.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -c api-nest-cli.json",
    "start:dev": "nest start  -c api-nest-cli.json --watch",
    "start:debug": "nest start -c api-nest-cli.json --debug --watch",
    "start:prod": "node dist/main",
    "birthday-greeter-send": "ts-node -r tsconfig-paths/register src/console.main.ts birthday-greeter-send",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
